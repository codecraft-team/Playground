{
  "Logging": {
    "IncludeScopes": true,
    "LogLevel": {
      "Default": "Warning",
      "Test": "Debug"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Literate" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      { "Name": "LiterateConsole" },
      { "Name": "Trace", "MinimumLevel":  "Warning"  }
    ],
    "Enrich": [ "FromLogContext" ],
    "Properties": {
      "Application": "Test"
    }
  }
}